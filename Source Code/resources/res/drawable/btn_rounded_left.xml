<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="#f58220"/>
            <padding android:left="-1dp"/>
            <corners android:topLeftRadius="2dp" android:bottomLeftRadius="2dp"/>
            <stroke android:width="1dp" android:color="#f58220"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#f58220"/>
            <padding android:left="-1dp"/>
            <corners android:topLeftRadius="2dp" android:bottomLeftRadius="2dp"/>
            <stroke android:width="1dp" android:color="#f58220"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#00000000"/>
            <padding android:left="-1dp"/>
            <corners android:topLeftRadius="2dp" android:bottomLeftRadius="2dp"/>
            <stroke android:width="1dp" android:color="#f58220"/>
        </shape>
    </item>
</selector>
