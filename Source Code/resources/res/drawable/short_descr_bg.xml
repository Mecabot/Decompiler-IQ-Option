<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/grey_blur_05"/>
            <corners android:bottomLeftRadius="@dimen/dp4" android:bottomRightRadius="@dimen/dp4"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/grey_blur_10"/>
            <corners android:bottomLeftRadius="@dimen/dp4" android:bottomRightRadius="@dimen/dp4"/>
        </shape>
    </item>
</selector>
