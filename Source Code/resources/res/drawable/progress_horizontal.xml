<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dp4"/>
            <solid android:color="#54000000"/>
            <gradient android:startColor="#00ffd300" android:endColor="#00ffcb00" android:angle="270" android:centerY="0.75" android:centerColor="#00ffb600"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp4"/>
                <gradient android:startColor="#f58220" android:endColor="#f58220" android:angle="270" android:centerY="0.75" android:centerColor="#f58220"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp4"/>
                <gradient android:startColor="#f58220" android:endColor="#f58220" android:angle="270" android:centerY="0.75" android:centerColor="#f58220"/>
            </shape>
        </clip>
    </item>
</layer-list>
