<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:left="@dimen/dp1" android:top="@dimen/dp2" android:right="@dimen/dp1" android:bottom="@dimen/dp2">
        <selector android:exitFadeDuration="@android:integer/config_mediumAnimTime">
            <item android:state_pressed="true">
                <shape>
                    <solid android:color="@color/grey_blur_30"/>
                    <corners android:radius="@dimen/withdraw_method_corners"/>
                </shape>
            </item>
            <item>
                <shape>
                    <solid android:color="@color/transparent"/>
                </shape>
            </item>
        </selector>
    </item>
</layer-list>
