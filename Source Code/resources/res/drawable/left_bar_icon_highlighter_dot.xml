<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:height="@dimen/dp11" android:width="@dimen/dp11">
        <shape android:shape="oval">
            <solid android:color="@color/dark_blue"/>
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="oval">
            <gradient android:startColor="@color/white" android:endColor="@color/transparent" android:type="radial" android:gradientRadius="@dimen/dp4"/>
        </shape>
    </item>
    <item android:gravity="center" android:height="@dimen/dp5" android:width="@dimen/dp5">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>
</layer-list>
