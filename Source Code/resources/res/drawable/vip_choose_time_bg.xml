<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp18"/>
            <solid android:color="@color/withdraw_green"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp18"/>
            <solid android:color="@color/withdraw_green"/>
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp18"/>
            <stroke android:width="@dimen/dp1" android:color="@color/grey_blur"/>
        </shape>
    </item>
</selector>
