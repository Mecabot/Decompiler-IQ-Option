<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true" android:right="1px" android:bottom="1px">
        <shape android:shape="rectangle">
            <solid android:color="#252d3f"/>
            <corners android:bottomRightRadius="@dimen/dp8"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#001b1e2b"/>
            <corners android:bottomRightRadius="@dimen/dp8"/>
        </shape>
    </item>
</selector>
