<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dp1" android:color="@color/transparent"/>
            <corners android:radius="@dimen/dp8"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dp1" android:color="@color/white_30"/>
            <corners android:radius="@dimen/dp8"/>
        </shape>
    </item>
</selector>
