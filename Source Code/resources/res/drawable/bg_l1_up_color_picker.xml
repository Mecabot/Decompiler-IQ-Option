<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/grey_blur_10"/>
            <corners android:radius="@dimen/dp4"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="#171e32"/>
            <stroke android:width="1dp" android:color="@color/white"/>
            <corners android:topLeftRadius="0px" android:topRightRadius="0px" android:bottomLeftRadius="@dimen/dp4" android:bottomRightRadius="@dimen/dp4"/>
        </shape>
    </item>
</selector>
