<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#3a3f4f"/>
            <corners android:topRightRadius="2dp" android:bottomRightRadius="2dp"/>
        </shape>
    </item>
    <item android:bottom="1px">
        <shape android:shape="rectangle">
            <solid android:color="#1e222d"/>
            <corners android:topRightRadius="2dp" android:bottomRightRadius="2dp"/>
        </shape>
    </item>
    <item android:top="1px" android:bottom="1px">
        <shape android:shape="rectangle">
            <solid android:color="#1e222d"/>
            <corners android:topRightRadius="2dp" android:bottomRightRadius="2dp"/>
        </shape>
    </item>
</layer-list>
