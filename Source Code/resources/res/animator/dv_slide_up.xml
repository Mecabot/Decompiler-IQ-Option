<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:ordering="sequentially">
    <set android:ordering="together">
        <objectAnimator android:interpolator="@android:interpolator/decelerate_cubic" android:duration="100" android:valueFrom="1000" android:valueTo="1000" android:valueType="floatType" android:propertyName="translationY"/>
        <objectAnimator android:interpolator="@android:interpolator/accelerate_decelerate" android:duration="100" android:valueFrom="0" android:valueTo="0" android:propertyName="alpha"/>
    </set>
    <set android:ordering="together">
        <objectAnimator android:interpolator="@android:interpolator/decelerate_cubic" android:duration="300" android:startOffset="200" android:valueFrom="1000" android:valueTo="0" android:valueType="floatType" android:propertyName="translationY"/>
        <objectAnimator android:interpolator="@android:interpolator/accelerate_decelerate" android:duration="300" android:valueFrom="0" android:valueTo="1" android:propertyName="alpha"/>
    </set>
</set>
