<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:color="@color/orange">
    <item android:id="@android:id/mask">
        <shape>
            <solid android:color="@color/orange"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape>
                    <stroke android:width="1dp" android:color="#cf9b9fa5"/>
                    <corners android:radius="10dp"/>
                </shape>
            </item>
            <item android:state_pressed="false">
                <shape>
                    <stroke android:width="1dp" android:color="#cf9b9fa5"/>
                    <corners android:radius="10dp"/>
                </shape>
            </item>
        </selector>
    </item>
</ripple>
